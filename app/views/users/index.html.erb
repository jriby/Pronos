<h1>Users#index</h1>

<ul>  
<% @users.each do |user| %> 
  <li><%= user.login %> - <%= user.nom %> - <%= user.prenom %> - <%= user.mail %> - <%= user.points %> - <%= user.valide %> - <%= link_to 'Edit', edit_user_path(user.login), :remote => true %></li>
<% end %>
</ul>

<p><%= link_to 'New User', new_user_path, :remote => true %></p>
